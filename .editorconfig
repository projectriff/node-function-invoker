root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
charset = utf-8
indent_style = space
indent_size = 4

[{package.json}]
indent_style = space
indent_size = 2

[{/node_modules/**,/codegen/**}]
end_of_line = ignore
insert_final_newline = ignore
trim_trailing_whitespace = ignore
charset = ignore
indent_style = ignore
indent_size = ignore
